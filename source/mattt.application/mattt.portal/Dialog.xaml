<Window x:Class="mattt.portal.Dialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dialog" Height="300" Width="300" x:Name="Self">
    <DockPanel>
        <TextBlock x:Name="uxStatus" DockPanel.Dock="Bottom"></TextBlock>
        <Button x:Name="uxReset">New Game</Button>
        <UniformGrid x:Name="PlayingField" DockPanel.Dock="Top">
        </UniformGrid>
        
    </DockPanel>
</Window>
